{% block body %}
    <div class="container-fluid">
        <div class="col-md-12" style="background-color:#ffffff;">
            <div class="modal-dialog">
                <div class="modal-content" style="background-color:#7ec441 ">
                    <div class="panel-heading" >
                        <div class="row-fluid user-row">
                            <img src="{{ asset('logo-nuevo-1.png') }}" class="img-responsive" alt="Conxole Admin"/>
                        </div>
                    </div>
                    <div class="panel-heading">
                        <h3 class="panel-title">Registrarse</h3>
                    </div>
                    <div class="panel-body">
                        <form action="{{ path('user_new') }}"  method="post">
                            {% if form_errors(form.username)%}
                                <div class="alert alert-warning" role="alert">{{ form_errors(form.username) }}</div>
                            {% endif %}
                            <fieldset>
                                <div class="form-group">
                                    <input class="form-control" placeholder="Correo" name="appbundle_user[username]" type="email" autofocus="" value="" id="appbundle_user_username" required="required">
                                </div>
                                <div class="form-group">
                                    <input class="form-control" placeholder="ContraseÃ±a" name="appbundle_user[password]" type="text" value="" id="appbundle_user_password">
                                </div>

                                {{ form_row(form._token) }}

                                <button type="submit" class="btn btn-sm btn-primary">Registrar</button>
                                {#<a href="javascript:;" class="btn btn-sm btn-success">Login</a>#}
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <hr>

    </div>
